{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "grok/grok-php-client",
    "description": "Community-Driven PHP SDK for Grok AI API Integration.",
    "keywords": [
        "Grok PHP SDK",
        "Grok AI API"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/dependencies-packagist/grok-php-client",
    "authors": [
        {
            "name": "jundayw",
            "email": "jundayw@126.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "baota/client": "^1.0",
        "qwen/qwen-php-client": "^1.0",
        "annotation/larscan": "^1.1",
        "open-ai/openai-php-client": "^0.10.3",
        "deepseek/deepseek-php-client": "^2.0",
        "ollama/ollama-php-client": "^1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
