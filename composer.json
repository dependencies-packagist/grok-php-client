{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "grok/grok-php-client",
    "description": "Community-Driven PHP SDK for Grok AI API Integration.",
    "keywords": [
        "Grok PHP SDK",
        "Grok AI API"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/dependencies-packagist/deepseek-php-client",
    "authors": [
        {
            "name": "jundayw",
            "email": "jundayw@126.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Grok\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
